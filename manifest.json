{
    "name": "Preeview",
    "description": "A google chrome extension for viewing links without clicking them",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/Preeview16.png",
            "32": "/images/Preeview32.png",
            "64": "/images/Preeview64.png",
            "128": "/images/Preeview128.png"
        }
    },
    "icons": {
        "16": "/images/Preeview16.png",
        "32": "/images/Preeview32.png",
        "64": "/images/Preeview64.png",
        "128": "/images/Preeview128.png"
    }
}